<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.kracr.iiitd.edu.in/ontology/apple#"
     xml:base="http://www.kracr.iiitd.edu.in/ontology/apple"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:AIRO="https://w3id.org/AIRO#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:time="http://www.w3.org/2006/time#"
     xmlns:apple="http://www.kracr.iiitd.edu.in/ontology/apple/"
     xmlns:event="http://w3id.org/daselab/onto/event#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:traffic="http://www.sensormeasurement.appspot.com/ont/transport/traffic#"
     xmlns:objectrole="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#"
     xmlns:trajectory="http://w3id.org/daselab/onto/trajectory#"
     xmlns:participation="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#"
     xmlns:coparticipation="http://www.ontologydesignpatterns.org/cp/owl/coparticipation.owl#">
    <owl:Ontology rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple">
        <owl:versionIRI rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/1.0"/>
        <terms:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Manohar Kumar</terms:contributor>
        <terms:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aisha Aijaz</terms:creator>
        <terms:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Raghava Mutharaju</terms:creator>
        <terms:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An Ontology for applied ethics and context to capture relevant ethical information that may provide insight to the morality of a course of action during a particular event.</terms:description>
        <terms:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ApplE: An Applied Ethics Ontology</terms:title>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Version 1.0 of the ApplE Ontology for Applied Ethics.</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/contributor -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/contributor"/>
    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/daselab/onto/event#hasInformationObject -->

    <owl:ObjectProperty rdf:about="http://w3id.org/daselab/onto/event#hasInformationObject">
        <owl:inverseOf rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/occursInEvent"/>
        <rdfs:domain rdf:resource="http://schema.org/Event"/>
        <rdfs:range rdf:resource="http://schema.mobivoc.org/Action"/>
        <rdfs:comment>Property relating Event to InformationObject in this pattern. The domain of this property is not restricted to Event  since it is possible that non-event to have an information object. The range is always InformationObject since it makes no sense to have a property named hasInformationObject to point to anything other than information object.

However, when reusing this concept from the EventCore ODP for the ApplE ontology, this property will link an event and an action.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/daselab/onto/event#subEventOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/daselab/onto/event#subEventOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://schema.org/Event"/>
        <rdfs:range rdf:resource="http://schema.org/Event"/>
        <rdfs:comment>Property expressing partonomic relation between two events. Domain and range are always Event.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/daselab/onto/trajectory#atPlace -->

    <owl:ObjectProperty rdf:about="http://w3id.org/daselab/onto/trajectory#atPlace">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://schema.org/Event"/>
        <rdfs:range rdf:resource="http://w3id.org/daselab/onto/trajectory#Place"/>
        <rdfs:comment>A property that describes where an action occurred.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/daselab/onto/trajectory#atTime -->

    <owl:ObjectProperty rdf:about="http://w3id.org/daselab/onto/trajectory#atTime">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://schema.org/Event"/>
        <rdfs:range rdf:resource="http://w3id.org/daselab/onto/trajectory#TimeEntity"/>
        <rdfs:comment>A property that describes when an action occurred.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple#describedBy -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple#describedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/describesEvent"/>
        <rdfs:domain rdf:resource="http://schema.org/Event"/>
        <rdfs:range rdf:resource="http://contextus.net/ontology/ontomedia/core/expression#Context"/>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple#hasDurationOfConsequence -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple#hasDurationOfConsequence">
        <rdfs:subPropertyOf rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/hasCharacteristicOfConsequence"/>
        <rdfs:domain rdf:resource="https://w3id.org/AIRO#Consequence"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/DurationOfConsequence"/>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple#hasSeverityOfConsequence -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple#hasSeverityOfConsequence">
        <rdfs:subPropertyOf rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/hasCharacteristicOfConsequence"/>
        <rdfs:domain rdf:resource="https://w3id.org/AIRO#Consequence"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/SeverityOfConsequence"/>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple#hasUtilityOfConsequence -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple#hasUtilityOfConsequence">
        <rdfs:subPropertyOf rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/hasCharacteristicOfConsequence"/>
        <rdfs:domain rdf:resource="https://w3id.org/AIRO#Consequence"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/UtilityOfConsequence"/>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple#violatedByAction -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple#violatedByAction">
        <owl:inverseOf rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/violatesEthicalPrinciple"/>
        <rdfs:domain rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/EthicalPrinciple"/>
        <rdfs:range rdf:resource="http://schema.mobivoc.org/Action"/>
        <rdfs:comment>This property describes the ethical principle that was violated by the occurrence of an action.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/adheresTo -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/adheresTo">
        <owl:inverseOf rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/appliesTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/AIRO#Domain"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/AppliedEthics"/>
        <rdfs:comment>A domain adheres to a specific type of applied ethics. For example, the medical domain adheres to bioethics. Again, this connection is not absolute. For example, an issue of the medical domain may also require adherance to the professional domain, when considering the treatment of nurses and doctors in large private hospitals.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/affects -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/affects">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://schema.mobivoc.org/Action"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/PassiveAgent"/>
        <rdfs:comment>Affects implies the verb in the statement &quot;an action affects a passive agent&quot; in whatever way, shape, or form.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/appliesPhilosophy -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/appliesPhilosophy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/AppliedEthics"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/AppliedEthicsPhilosophy"/>
        <rdfs:comment>A type of applied ethics may apply a type of philosophy.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/appliesTo -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/appliesTo">
        <rdfs:domain rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/AppliedEthics"/>
        <rdfs:range rdf:resource="https://w3id.org/AIRO#Domain"/>
        <rdfs:comment>Inverse of the adheresTo object property. This property implies that a type of applied ethics applies to a certain domain.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/associatedWith -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/associatedWith">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/NormativeEthics"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/EthicalPrinciple"/>
        <rdfs:comment>Types of normative ethics may be associated with certain ethical concepts.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/causesEthicalIssue -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/causesEthicalIssue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/AIRO#Consequence"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/EthicalIssue"/>
        <rdfs:comment>The consequence of an action may cause an ethical issue. (This is not to be confused with the fact that some may consider a consequence to be an ethical issue, as these are two separate concepts. For example: the firing of an employee on the basis of their race is a consequence of the &apos;firing&apos; action. However, the firing is not the ethical issue. The issue is a business ethical issue: Discrimination in the workplace.)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/describesEvent -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/describesEvent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://contextus.net/ontology/ontomedia/core/expression#Context"/>
        <rdfs:range rdf:resource="http://schema.org/Event"/>
        <rdfs:comment>The context describes an event that has occured.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/hasCharacteristicOfConsequence -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/hasCharacteristicOfConsequence">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/AIRO#Consequence"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/CharacteristicOfConsequence"/>
        <rdfs:comment>This object property describes the characteristic of the consequences of an action.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/hasMoralIntention -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/hasMoralIntention">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/ActiveAgent"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/MoralIntention"/>
        <rdfs:comment>An agent, when doing an action, has moral intention behind the action.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/occursInDomain -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/occursInDomain">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://schema.org/Event"/>
        <rdfs:range rdf:resource="https://w3id.org/AIRO#Domain"/>
        <rdfs:comment>An event occurs in some domain.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/occursInEvent -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/occursInEvent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://schema.mobivoc.org/Action"/>
        <rdfs:range rdf:resource="http://schema.org/Event"/>
        <rdfs:comment>An action occurs during some event.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/resolvedBy -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/resolvedBy">
        <owl:inverseOf rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/resolves"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/EthicalIssue"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/AppliedEthicsPhilosophy"/>
        <rdfs:comment>An ethical issue is resolved by an applied ethics philosophy. Here &apos;resolves&apos; implies the resolution of the issue.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/resolves -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/resolves">
        <rdfs:domain rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/AppliedEthicsPhilosophy"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/EthicalIssue"/>
        <rdfs:comment>Inverse of the resolvedBy property. An applied ethics philosophy resolves an applied ethics issue. Although the term is assertive (resolves), the property demostrates that a certain property may be used to resolve an ethical issue.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/upholdsEthicalPrinciple -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/upholdsEthicalPrinciple">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://schema.mobivoc.org/Action"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/EthicalPrinciple"/>
        <rdfs:comment>An action may uphold certain ethical principles.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/apple/violatesEthicalPrinciple -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple/violatesEthicalPrinciple">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://schema.mobivoc.org/Action"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/EthicalPrinciple"/>
        <rdfs:comment>An action may violate certain ethical principles.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/coparticipation.owl#coparticipatesWith -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/coparticipation.owl#coparticipatesWith">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment>Agents may co-participate with each other in an event in any capacity (active or passive).</rdfs:comment>
        <rdfs:isDefinedBy>http://www.ontologydesignpatterns.org/cp/owl/coparticipation.owl#</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole">
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#isRoleOf"/>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Role"/>
        <rdfs:comment>A relation between an object and a role, e.g. the person &apos;John&apos; has role &apos;student&apos;.</rdfs:comment>
        <rdfs:isDefinedBy>http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#isRoleOf -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#isRoleOf">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Role"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment>A relation between an object and a role, e.g. the &apos;student&apos; is the role of &apos;John&apos;.</rdfs:comment>
        <rdfs:isDefinedBy>http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant">
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://schema.org/Event"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment>Inverse of the &quot;isParticipantIn&quot; object property from the participant Ontology Design Pattern. Shows an event which has a participating agent.</rdfs:comment>
        <rdfs:isDefinedBy>http://www.ontologydesignpatterns.org/cp/owl/participation.owl#</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:range rdf:resource="http://schema.org/Event"/>
        <rdfs:comment>An agent is a participant in an event.</rdfs:comment>
