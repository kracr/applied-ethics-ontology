    <!-- http://www.kracr.iiitd.edu.in/ontology/apple#Bioethics -->

    <owl:NamedIndividual rdf:about="http://www.kracr.iiitd.edu.in/ontology/apple#Bioethics">
        <rdf:type rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/Ethics"/>
        <rdf:type rdf:resource="https://w3id.org/skgo/modsci#Bioethics"/>
        <apple:appliesPhilosophy rdf:resource="http://www.kracr.iiitd.edu.in/ontology/apple/Principlism"/>
        <rdfs:comment>The study of ethical, social, and legal issues that arise in biomedicine and biomedical research</rdfs:comment>
    </owl:NamedIndividual>
    


