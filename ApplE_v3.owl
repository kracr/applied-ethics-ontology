        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/associatedWith"/>
                <owl:someValuesFrom rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/EthicalPrinciples"/>
            </owl:Restriction>
