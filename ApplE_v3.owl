        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/ActiveAgent"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/MoralIntention"/>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/appl-e/occursInDomain -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/appl-e/occursInDomain">
