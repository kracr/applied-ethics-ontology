    <!-- http://www.kracr.iiitd.edu.in/ontology/appl-e/hasScore -->

    <owl:DatatypeProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/appl-e/hasScore">
        <owl:propertyDisjointWith rdf:resource="http://www.w3.org/2006/time#hasBeginning"/>
        <owl:propertyDisjointWith rdf:resource="http://www.w3.org/2006/time#hasEnd"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2006/time#hasBeginning -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2006/time#hasBeginning">
        <rdfs:domain rdf:resource="http://schema.org/Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2006/time#hasEnd -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2006/time#hasEnd">
        <rdfs:domain rdf:resource="http://schema.org/Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://contextus.net/ontology/ontomedia/core/expression#Context -->

    <owl:Class rdf:about="http://contextus.net/ontology/ontomedia/core/expression#Context">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/describesEvent"/>
                <owl:someValuesFrom rdf:resource="http://schema.org/Event"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/Ethics"/>
        <rdfs:comment xml:lang="en">This class represents the context in which an event or entity exists. (From OC Ontology: https://lov.linkeddata.es/dataset/lov/vocabs/oc).</rdfs:comment>
    </owl:Class>
    


    <!-- http://schema.mobivoc.org/Action -->

    <owl:Class rdf:about="http://schema.mobivoc.org/Action">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/affects"/>
                <owl:someValuesFrom rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/PassiveAgent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/occursInEvent"/>
                <owl:someValuesFrom rdf:resource="http://schema.org/Event"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/AIRO#hasConsequence"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/AIRO#Consequence"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment xml:lang="en">Action that is done. (From schema: https://lov.linkeddata.es/dataset/lov/vocabs/mv)</rdfs:comment>
    </owl:Class>
    


    <!-- http://schema.org/Event -->

    <owl:Class rdf:about="http://schema.org/Event">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://contextus.net/ontology/ontomedia/core/expression#involves"/>
                <owl:someValuesFrom rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://data.semanticweb.org/ns/swc/ontology#hasLocation"/>
                <owl:someValuesFrom rdf:resource="http://schema.org/Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/occursInDomain"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/AIRO#Domain"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2006/time#hasBeginning"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2006/time#hasEnd"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An event happening at a certain time and location,&gt; property. Repeated events may be structured as separate Event objects. (From schema: https://lov.linkeddata.es/dataset/lov/vocabs/schema)</rdfs:comment>
    </owl:Class>
    


    <!-- http://schema.org/Place -->

    <owl:Class rdf:about="http://schema.org/Place">
        <rdfs:comment>Entities that have a somewhat fixed, physical extension. (From schema: https://schema.org/Place)</rdfs:comment>
        <rdfs:label>Place</rdfs:label>
    </owl:Class>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/appl-e/AcademicEthics -->

    <owl:Class rdf:about="http://www.kracr.iiitd.edu.in/ontology/appl-e/AcademicEthics">
        <rdfs:subClassOf rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/ProfessionalEthics"/>
        <owl:disjointWith rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/BusinessEthics"/>
    </owl:Class>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/appl-e/ActiveAgent -->

    <owl:Class rdf:about="http://www.kracr.iiitd.edu.in/ontology/appl-e/ActiveAgent">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/hasMoralIntention"/>
                <owl:someValuesFrom rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/MoralIntention"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.sensormeasurement.appspot.com/ont/transport/traffic#doesAction"/>
                <owl:someValuesFrom rdf:resource="http://schema.mobivoc.org/Action"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An active agent is an agent that does the action in an event.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/appl-e/AppliedEthics -->

    <owl:Class rdf:about="http://www.kracr.iiitd.edu.in/ontology/appl-e/AppliedEthics">
        <rdfs:subClassOf rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/Ethics"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/appliesPhilosophy"/>
                <owl:someValuesFrom rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/AppliedEthicsPhilosophy"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/appl-e/AppliedEthicsPhilosophy -->

    <owl:Class rdf:about="http://www.kracr.iiitd.edu.in/ontology/appl-e/AppliedEthicsPhilosophy">
        <rdfs:comment xml:lang="en">A philosophy that acts as a guiding principle to make ethical decisions pertaining to certain real-world domains.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/appl-e/BioethicsIssue -->

    <owl:Class rdf:about="http://www.kracr.iiitd.edu.in/ontology/appl-e/BioethicsIssue">
        <rdfs:subClassOf rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/EthicalIssue"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/resolvedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/BioethicsPhilosophy"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/appl-e/BioethicsPhilosophy -->

    <owl:Class rdf:about="http://www.kracr.iiitd.edu.in/ontology/appl-e/BioethicsPhilosophy">
        <rdfs:subClassOf rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/AppliedEthicsPhilosophy"/>
        <rdfs:comment xml:lang="en">A philosophy that acts as a guiding principle to make ethical decisions pertaining to bioethics in a real-world medical domain.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/appl-e/BusinessEthicalIssue -->

    <owl:Class rdf:about="http://www.kracr.iiitd.edu.in/ontology/appl-e/BusinessEthicalIssue">
        <rdfs:subClassOf rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/EthicalIssue"/>
        <rdfs:subClassOf>
