        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://contextus.net/ontology/ontomedia/core/expression#Context"/>
        <rdfs:range rdf:resource="http://schema.org/Event"/>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/appl-e/hasCharacteristicOfConsequence -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/appl-e/hasCharacteristicOfConsequence">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/AIRO#Consequence"/>
        <rdfs:range rdf:resource="http://www.kracr.iiitd.edu.in/ontology/appl-e/CharacteristicOfConsequence"/>
    </owl:ObjectProperty>
    


    <!-- http://www.kracr.iiitd.edu.in/ontology/appl-e/hasMoralIntention -->

    <owl:ObjectProperty rdf:about="http://www.kracr.iiitd.edu.in/ontology/appl-e/hasMoralIntention">
